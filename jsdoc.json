{
  "source": {
    "includePattern": ".+\\.ts(doc|x)?$"
  },
  "plugins": [
    "plugins/markdown",
    "node_modules/jsdoc-babel",
    "node_modules/jsdoc-strip-async-await"
  ],
  "babel": {
    "extensions": ["ts"],
    "babelrc": false,
    "presets": [
      ["@babel/preset-env", { "targets": { "node": true } }],
      "@babel/preset-typescript"
    ]
  }
}